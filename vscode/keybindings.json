// Place your key bindings in this file to override the defaultsauto[]
[
    {
        "key": "ctrl+x [",
        "command": "cursorTop",
        "when": "textInputFocus"
    },
    {
        "key": "ctrl+x ]",
        "command": "cursorBottom",
        "when": "textInputFocus"
    },
    {
        "key": "ctrl+A",
        "command": "cursorLineStart",
        "when": "textInputFocus"
    },
    {
        "key": "ctrl+E",
        "command": "cursorLineEnd",
        "when": "textInputFocus"
    }
]